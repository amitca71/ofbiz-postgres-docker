    <!-- ========================================================= -->
    <!-- Facts                                                     -->
    <!-- ========================================================= -->
     <entity entity-name="CardFinancialFact" package-name="org.ofbiz.bi.fact.CardFinancial" title="CardFinancial transactions Fact">
        <description>A transaction CardFinancial fact entity with transaction item grain.</description>
        <field name="finId" type="id-ne"><description> degenerate dimension</description></field>
        <field name="recordType" type="id-ne"><description> record type </description></field>
        <field name="txnCodeType" type="id-ne"><description>transaction code type </description></field>
        <field name="localDateDimId" type="id-ne"><description>transaction date in local</description></field>
        <field name="inventoryItemId" type="id-ne"><description>inventory item id</description></field>
        <field name="settlementDate" type="date-time"><description>settlement date time</description></field>
        <field name="productDimId" type="id-ne"><description> product dimension</description></field>
        <field name="dealerDimId" type="id-ne"><description>dealer dimension  </description></field>
        <field name="direction" type="id-ne"><description>transaction direction </description></field>
        <field name="mccDimId"  type="id-ne"><description> merchant code</description></field>

        <field name="origTxnAmount" type="currency-precise">
            <description>transaction amount in orig currency</description>
        </field>
        <field name="origCurrencyDimId" type="id-ne"></field>      
        <field name="settlementAmount" type="currency-precise">
            <description>settlement amount</description>
        </field>
        <field name="fee" type="currency-precise">
            <description>transaction fee in local (ILS) currency</description>
        </field>  
        <field name="feeDirection" type="id-ne">
            <description>transaction fee direction</description>
        </field>   
       <field name="feeAmt" type="currency-precise">
