        <field name="dimensionId" type="id-ne">
            <description>Unique identifier of the dealer sold the card</description>
        </field>
        <field name="dealerPartyId" type="id-ne">
            <description>The facility oenwr partyId. From Facility.ownerPartyId</description>
        </field>
